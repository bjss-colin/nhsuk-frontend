
<nav role="navigation" class="nhsuk-nav-sibling 
{%- if params.classes %} {{ params.classes }}{% endif %}" {%- for attribute, value in params.attributes %} {{attribute}}="{{value}}"{% endfor %}>
  <ul class="nhsuk-nav-sibling__list">
    {%- for item in params.items %}
      {%- if item.active %}
        <li class="nhsuk-nav-sibling__item is-active" >
          {{ item.text }}
        </li>
      {%- else %}
        <li class="nhsuk-nav-sibling__item">
          <a class="nhsuk-nav-sibling__link" href="{{ item.href }}">
            {{ item.text }}
          </a>
        </li>
      {%- endif %}
    {%- endfor %}
  </ul>
</nav>
